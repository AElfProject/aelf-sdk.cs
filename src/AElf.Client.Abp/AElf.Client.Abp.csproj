<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <PackageId>AElf.Client.Abp</PackageId>
        <PackageDescription>AElf C# sdk with abp module.</PackageDescription>
        <Company>AElf</Company>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="Volo.Abp.Autofac" Version="5.2.2" />
        <PackageReference Include="Volo.Abp.AutoMapper" Version="5.2.2" />
    </ItemGroup>
    
    <ItemGroup>
      <ProjectReference Include="..\AElf.Client\AElf.Client.csproj" />
    </ItemGroup>

</Project>
